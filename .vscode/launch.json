{
	"version": "0.2.0",
	"configurations": [{
			"type": "chrome",
			"request": "launch",
			"name": "Launch Chrome",
			"webRoot": "${workspaceRoot}",
			"file": "${workspaceRoot}/src/pages/index.html",
			"sourceMaps": true
		},
		{
			"type": "chrome",
			"request": "launch",
			"name": "Launch Chrome against localhost",
			"url": "http://localhost:8080/src/pages/index.html",
			"webRoot": "${workspaceRoot}",
			"sourceMaps": true
		},
		{
			"type": "chrome",
			"request": "attach",
			"name": "Attach to Chrome",
			"port": 9222,
			"webRoot": "${workspaceRoot}"
		}
	]
}